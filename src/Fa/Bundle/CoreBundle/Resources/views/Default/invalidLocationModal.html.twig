<a class="close-reveal-modal">&#215;</a>
<h5 class="modal-title">
    {{'Invalid location'|trans({}, 'frontend-invalid-location')}}
</h5>
<div class="modal-content remove-modal">
    <div class="row">
        <div class="columns">
            <h5>{{ 'Sorry no location available with such name.'|trans({}, 'frontend-invalid-location') }}</h5>
        </div>
    </div>
    <div class="mt20 remove-item-btn">
        <div class="row">
            <div class="large-24 columns">
                <button type="button" class="button secondary-btn-4 thin-btn expand" onclick="closeRevealModel('#{{ modalId }}');">{{'Close'|trans({}, 'frontend-invalid-location')}}</button>
            </div>
        </div>
    </div>
</div>