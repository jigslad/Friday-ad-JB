{% if homePopularImagesArray|length %}
    <div id="homepage-populars" class="row popular-items">
        {% for homePopularImage in homePopularImagesArray %}
            <div class="large-6 columns">
                <div class="item-box" style="background-image: url('{{ homePopularImage['bg_image'] }}');background-size: cover;background-position: center;">
                    <a href="{{ homePopularImage['url'] }}" class="tricky-link">{{ homePopularImage['bg_image'] }}</a>
                    <div class="overlay"></div>
                    <div class="popular-graphic"><img src="{{ homePopularImage['overlay_image'] }}" alt="" /></div>
                    <!-- <div class="parallax-window" data-speed="0.6" data-parallax="scroll" data-z-index="1" data-natural-width="250" data-natural-height="290" data-image-src="https://fad-images.friday-ad.co.uk/uploads/homepopular/5ebcd574f1196.png"></div> -->
                </div>
            </div>
        {% endfor %}
    </div>
{% endif %}
